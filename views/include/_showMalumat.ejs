<% for(var i = 0; i < malumatlar.length; i++) { %>
<tr>
    
	<td><%= malumatlar[i].malumatSual %></td>
	<td><%= malumatlar[i].kitapAdi %></td>

	
    <td>
        <a href="/update-malumat?id=<%= malumatlar[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%=malumatlar[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>